{
	"name": "normal",
	"default": true,
	"commands": [
		{ "keys": [  "m" ], "partial": ["mark"],"actions":[ { "action": "SetMark" } ]},
		{ "keys": [  "'" ], "partial": ["mark"], "actions":[ { "action": "Move" } ]},
		{ "keys": [  "d" ], "partial": ["object", "range", "move"], "actions":[ { "action": "Delete" } ]},
		{ "keys": [  "y" ], "partial": ["object", "range", "move"], "actions":[ { "action": "Yank" } ]},
		{ "keys": [  "c" ], "partial": ["object", "range", "move"], "actions":[ { "action": "Delete" }, { "action": "ChangeMode", "target": "insert" } ]},
		{ "keys": [  "q" ], "partial": ["register"], "actions":[ { "action": "StartRecordMacro" } ]},
		{ "keys": [  "@" ], "partial": ["macro"], "actions":[ { "action": "RunMacro" } ]},

		{ "keys": [  "<ctrl-v>" ], "actions":[ { "action": "ChangeMode", "target": "visualblock" } ]},
		{ "keys": [  ":" ], "actions":[ { "action": "ChangeMode", "target": "colon" } ]},
		{ "keys": [  "/" ], "actions":[ { "action": "ChangeMode", "target": "search" } ]},
		{ "keys": [  "i" ], "actions":[ { "action": "ChangeMode", "target": "insert" } ]},
		{ "keys": [  "V" ], "actions":[ { "action": "ChangeMode", "target": "visualLine" } ]},
		{ "keys": [  "v" ], "actions":[ { "action": "ChangeMode", "target": "visual" } ]},

		{ "keys": [  "$" ], "actions":[ { "action": "Move", "target": "eol" } ]},
		{ "keys": [  "0" ], "actions":[ { "action": "Move", "target": "bol" } ]},
		{ "keys": [  "^" ], "actions":[ { "action": "Move", "target": "bol-not-empty" } ]},
		{ "keys": [  "b" ], "actions":[ { "action": "MovePrev", "target": "word" } ]},
		{ "keys": [  "%" ], "actions":[ { "action": "Move", "target": "pair-match" } ]},
		{ "keys": [  "e" ], "actions":[ { "action": "MoveEnd", "target": "word" } ]},
		{ "keys": [  "G" ], "actions":[ { "action": "Move", "target": "document-end" } ]},
		{ "keys": [  "g", "_" ], "actions":[ { "action": "Move", "target": "eol-not-empty" } ]},
		{ "keys": [  "g", "g" ], "actions":[ { "action": "Move", "target": "document-begin" } ]},
		{ "keys": [  "h" ], "actions":[ { "action": "MovePrev", "target": "char" } ]},
		{ "keys": [  "j" ], "actions":[ { "action": "Move", "target": "line" } ]},
		{ "keys": [  "k" ], "actions":[ { "action": "MovePrev", "target": "line" } ]},
		{ "keys": [  "l" ], "actions":[ { "action": "Move", "target": "char" } ]},
		{ "keys": [  "N" ], "actions":[ { "action": "MovePrev", "target": "search" } ]},
		{ "keys": [  "n" ], "actions":[ { "action": "Move", "target": "search" } ]},
		{ "keys": [  "W" ], "actions":[ { "action": "Move", "target": "full-word" } ]},
		{ "keys": [  "w" ], "actions":[ { "action": "Move", "target": "word" } ]},

		{ "keys": [  "a" ], "actions":[ { "action": "ChangeMode", "target": "insert" }, { "action": "Move", "target": "char" } ]},
		{ "keys": [  "A" ], "actions":[ { "action": "Move", "target": "eol" }, { "action": "ChangeMode", "target": "insert" } ]},
		{ "keys": [  "I" ], "actions":[ { "action": "Move", "target": "bol-not-empty" }, { "action": "ChangeMode", "target": "insert" } ]},

		{ "keys": [  "D" ], "actions":[ { "action": "DeleteTo", "target": "eol" } ]},
		{ "keys": [  "d", "d" ], "actions":[ { "action": "Delete", "target": "line" } ]},
		{ "keys": [  "x" ], "actions":[ { "action": "Delete", "target": "char" } ]},
		{ "keys": [  "X" ], "actions":[ { "action": "Delete", "target": "char" }, { "action": "MovePrev", "target": "char" } ]},

		{ "keys": [  "O" ], "actions":[ { "action": "Insert", "target": "line-above" }, { "action": "ChangeMode", "target": "insert" } ]},
		{ "keys": [  "o" ], "actions":[ { "action": "Insert", "target": "line" }, { "action": "ChangeMode", "target": "insert" } ]},

		{ "keys": [  ">", ">" ], "actions":[ { "action": "indent", "target": "line" }, { "action": "MoveBeginNotEmpty", "target": "line" } ]},
		{ "keys": [  "<", "<" ], "actions":[ { "action": "outdent", "target": "line" }, { "action": "MoveBeginNotEmpty", "target": "line" } ]},

		{ "keys": [  "H" ], "actions":[ { "action": "Move", "target": "screen-begin" } ]},
		{ "keys": [  "L" ], "actions":[ { "action": "Move", "target": "screen-end" } ]},
		{ "keys": [  "M" ], "actions":[ { "action": "Move", "target": "screen-center" }, { "action": "Move", "target": "bol-not-empty" } ]},
		{ "keys": [  "zz" ], "actions":[ { "action": "Move", "target": "screen-center" } ]},
		{ "keys": [  "<ctrl-d>" ], "actions":[ { "action": "Scroll", "target": "down" } ]},
		{ "keys": [  "<ctrl-u>" ], "actions":[ { "action": "Scroll", "target": "up" } ]},

		{ "keys": [  "<ctrl-r>" ], "actions":[ { "action": "Redo" } ]},
		{ "keys": [  "u" ], "actions":[ { "action": "Undo" } ]},
		{ "keys": [  "p" ], "actions":[ { "action": "Paste" } ]},
		{ "keys": [  "y", "y" ], "actions":[ { "action": "Yank", "target": "line" } ]}
	]
}

