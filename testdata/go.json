[
	{
		"group": "comment.multiline",
		"type": "region",
		"start": "/\\*",
		"end": "\\*/",
		"multiline": true
	},
	{
		"group": "comment",
		"type": "region",
		"start": "//",
		"end": "$"
	},
	{
		"group": "constant.string.rune",
		"type": "region",
		"same": "'",
		"skip": "\\\\."
	},
	{
		"group": "constant.string",
		"type": "region",
		"same": "`",
		"multiline": true
	},
	{
		"group": "constant.string",
		"type": "region",
		"same": "\"",
		"skip": "\\\\."
	},
	{
		"group": "constant.number",
		"type": "match",
		"match": "\\b([0-9]+|0x[0-9a-fA-F]*)\\b|'.'"
	},
	{
		"group": "symbol.brackets",
		"type": "match",
		"match": "(\\[|\\])"
	},
	{
		"group": "symbol.brackets",
		"type": "match",
		"match": "(\\(|\\))"
	},
	{
		"group": "symbol.brackets",
		"type": "match",
		"match": "(\\{|\\})"
	},
	{
		"group": "constant.bool",
		"type": "keyword",
		"keywords": ["true", "false", "nil"]
	},
	{
		"group": "type.keyword",
		"type": "keyword",
		"keywords": ["struct"]
	},
	{
		"group": "type",
		"type": "keyword",
		"keywords": ["uintptr", "byte", "rune", "string", "interface", "bool", "map", "chan", "error"]
	},
	{
		"group": "type",
		"type": "keyword",
		"keywords": ["int8", "int16", "int32", "int64", "uint8", "uint16", "uint32", "uint64",
			"float32", "float64", "complex64", "complex128"]
	},
	{
		"group": "symbol",
		"type": "match",
		"match": "(,|\\.)"
	},
	{
		"group": "symbol.operator",
		"type": "match",
		"match": "[-+/*=<>!~%&|^]|:="
	},
	{
		"group": "preproc",
		"type": "keyword",
		"keywords": ["package", "import", "const", "var", "type", "struct", "func", "defer", "iota",
			"make", "new", "copy", "len", "cap", "panic", "append", "close", "delete", "print", "recover"]
	},
	{
		"group": "statement",
		"type": "keyword",
		"keywords": ["else", "for", "if", "switch", "select"]
	},
	{
		"group": "special",
		"type": "keyword",
		"keywords": ["break", "case", "continue", "default", "go", "goto", "range", "return", "println", "fallthrough"]
	}
]
